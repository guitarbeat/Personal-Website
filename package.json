{
  "name": "portfolio",
  "version": "0.1.0",
  "homepage": "https://aaronwoods.info/",
  "private": true,
  "engines": {
    "node": "22.x",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "personal website"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "unused-imports/no-unused-imports": "error"
    }
  },
  "scripts": {
    "start": "craco start --config config/craco.config.js",
    "dev": "craco start --config config/craco.config.js",
    "build": "craco build --config config/craco.config.js",
    "test": "craco test --config config/craco.config.js --passWithNoTests",
    "eject": "react-scripts eject",
    "format": "biome format ./src --write",
    "lint": "biome lint ./src",
    "check": "biome check --write .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "sass:check": "sass --no-source-map --no-error-css --verbose src:public/build/css",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "lint:md:fix": "markdownlint-cli2 --fix \"**/*.md\" \"#node_modules\"",
    "compress-images": "node scripts/compress-images.js",
    "build:dev": "vite build --mode development"
  },
  "dependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-private-methods": "^7.28.6",
    "@eslint/config-array": "^0.23.0",
    "@eslint/object-schema": "^3.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.8",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "@vitejs/plugin-react": "^5.1.4",
    "ajv": "^8.18.0",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.13.2",
    "canvas-confetti": "^1.9.4",
    "chalk": "^5.6.2",
    "chroma-js": "^3.2.0",
    "clean-css": "^5.3.3",
    "compression-webpack-plugin": "^11.1.0",
    "cors": "^2.8.5",
    "customize-cra": "^1.0.0",
    "deadfile": "^2.1.1",
    "eslint-config-react-app": "^7.0.1",
    "express": "^5.2.1",
    "framer-motion": "^12.27.0",
    "gsap": "^3.14.2",
    "lovable-tagger": "^1.1.13",
    "moment": "^2.30.1",
    "npm-run-all": "^4.1.5",
    "ogl": "^1.0.11",
    "open": "^10.1.0",
    "postcss-cli": "^11.0.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-db-google-sheets": "^3.0.0",
    "react-dom": "^19.2.3",
    "react-intersection-observer": "^10.0.2",
    "react-router-dom": "^7.12.0",
    "rimraf": "^6.1.3",
    "sass": "^1.77.8",
    "styled-components": "^6.3.8",
    "terser-webpack-plugin": "^5.3.16",
    "tone": "^15.1.22",
    "vite": "^7.3.1",
    "web-vitals": "^4.2.4"
  },
  "bin": {
    "deadfile": "./deadfile.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@biomejs/biome": "^2.4.0",
    "@craco/craco": "^7.1.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.1",
    "@types/chroma-js": "^3.1.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@types/prop-types": "^15.7.15",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "babel-eslint": "^10.1.0",
    "css-select": "^6.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest-watch-typeahead": "^2.2.2",
    "knip": "^5.82.0",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.21.0",
    "npm-check": "^6.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^13.3.2",
    "stylelint": "^17.3.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^7.0.0",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vercel": "^50.4.5"
  },
  "sassConfig": {
    "api": "modern",
    "sourceMap": true,
    "sourceMapEmbed": true
  },
  "resolutions": {
    "sass-loader": "^13.3.2",
    "postcss": "^8.5.6",
    "svgo": "^4.0.0",
    "css-select": "^6.0.0",
    "@svgr/webpack": "^8.1.0",
    "resolve-url-loader": "^5.0.0"
  },
  "overrides": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/guitarbeat/Personal-Website"
  }
}
