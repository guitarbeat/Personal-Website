{
  "name": "portfolio",
  "version": "0.1.0",
  "homepage": "https://aaronwoods.info/",
  "private": true,
  "keywords": [
    "personal website"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "start": "craco start",
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "format": "biome format ./src --write",
    "lint": "biome lint ./src",
    "check": "biome check --write .",
    "predeploy": "npm run build",
    "sass:check": "sass --no-source-map --no-error-css --verbose src:public/build/css"
  },
  "dependencies": {
    "@types/react": "^19.0.10",
    "canvas-confetti": "^1.9.2",
    "chroma-js": "^2.4.2",
    "eslint-config-react-app": "^7.0.1",
    "framer-motion": "^11.15.0",
    "moment": "^2.29.4",
    "ogl": "^1.0.11",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-db-google-sheets": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.0",
    "react-router-dom": "^6.23.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.14",
    "tone": "^14.8.39"
  },
  "bin": {
    "deadfile": "./deadfile.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.3",
    "@craco/craco": "^7.1.0",
    "eslint": "^8.56.0",
    "sass": "^1.83.1"
  },
  "sassConfig": {
    "api": "modern",
    "sourceMap": true,
    "sourceMapEmbed": true
  },
  "resolutions": {
    "sass-loader": "^13.3.2",
    "got": "^11.8.5",
    "nth-check": "^2.0.1",
    "postcss": "^8.4.31",
    "svgo": "^2.8.0",
    "css-select": "^4.0.0",
    "@svgr/webpack": "^6.5.1",
    "resolve-url-loader": "^5.0.0",
    "package-json": "^7.0.0",
    "latest-version": "^6.0.0",
    "update-notifier": "^6.0.0"
  },
  "overrides": {},
  "engines": {
    "node": "18.x",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guitarbeat/Personal-Website"
  }
}
